#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    char c;
    int n=0,word=0;
    while((c=getchar())!='\n')
    {
        if(c==' ')
            word=0;
        else if(word==0)
        {
            word=1;
            n++;
        }
    }
    printf("%d\n",n);

    return 0;
}
