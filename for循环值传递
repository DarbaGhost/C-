int main()
{
    int x[3],i,j,k;
  for(i=0;i<3;i++) x[i]=0;

  k=2;
 for(i=0;i<k;i++)
   {
     printf("第%d次\n",i);
     for(j=0;j<k;j++)
     {
        x[j]=x[j]+1;
      printf("%d %d\n",x[0],x[1]);
     }
     printf("...............\n");
   }

}
